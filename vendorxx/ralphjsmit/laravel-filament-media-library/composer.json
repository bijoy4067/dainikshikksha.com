{
    "name": "ralphjsmit/laravel-filament-media-library",
    "description": "A convenient media library for Filament Admin.",
    "keywords": [
        "ralphjsmit",
        "laravel",
        "laravel-filament-cms"
    ],
    "homepage": "https://github.com/ralphjsmit/laravel-filament-media-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph J. Smit",
            "email": "rjs@ralphjsmit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "filament/spatie-laravel-tags-plugin": "^3.0",
        "illuminate/contracts": "^8.78.1|^9.0|^10.0",
        "php-ffmpeg/php-ffmpeg": "^1.0",
        "ralphjsmit/laravel-helpers": "^1.8.1",
        "spatie/laravel-collection-macros": "^7.10",
        "spatie/laravel-medialibrary": "^9.0|^10.0.3|^11.0",
        "spatie/laravel-package-tools": "^1.11.0",
        "spatie/laravel-tags": "^4.3",
        "spatie/pdf-to-image": "^2.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^5.10|^6.1|^7.0",
        "orchestra/testbench": "^6.22|^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.2",
        "phpunit/phpunit": "^9.5|^10.0",
        "ralphjsmit/pest-plugin-livewire": "^1.2",
        "spatie/laravel-ray": "^1.29",
        "spatie/pest-plugin-test-time": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RalphJSmit\\Filament\\MediaLibrary\\": "src",
            "RalphJSmit\\Filament\\MediaLibrary\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RalphJSmit\\Filament\\MediaLibrary\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "ralphjsmit/packages": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RalphJSmit\\Filament\\MediaLibrary\\FilamentMediaLibraryServiceProvider"
            ],
            "aliases": {
                "MediaLibrary": "RalphJSmit\\Filament\\MediaLibrary\\Facades\\MediaLibrary"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ]
}
